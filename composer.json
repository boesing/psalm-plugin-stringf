{
    "name": "boesing/psalm-plugin-stringf",
    "type": "psalm-plugin",
    "description": "Psalm plugin to work with `sprintf`, `printf`, `sscanf` and `fscanf`.",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.4 || ~8.0.0",
        "vimeo/psalm": "^4.8"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "doctrine/coding-standard": "^9.0",
        "codeception/module-phpbrowser": "^1.0",
        "codeception/module-asserts": "^1.3",
        "weirdan/codeception-psalm-module": "^0.13.1"
    },
    "extra": {
        "psalm": {
            "pluginClass": "Boesing\\PsalPluginStringf\\Plugin"
        }
    },
    "autoload": {
        "psr-4": {
            "Boesing\\PsalmPluginStringf\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "codeception run",
        "analyze": "psalm --no-cache"
    }
}
